    "gulp-load-plugins": "^1.0.0-rc.1",
    "gulp-util": "^3.0.6",
    "jasmine-core": "^2.3.4",
    "jshint-stylish": "^2.0.1",
    "karma": "^0.13.10",
    "karma-chrome-launcher": "^0.2.0",
    "karma-coverage": "^0.5.2",
